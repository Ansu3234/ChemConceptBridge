{
  "name": "frontend",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@fontsource/poppins": "^5.2.6",
    "@testing-library/dom": "^10.4.1",
    "@testing-library/jest-dom": "^6.6.4",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^13.5.0",
    "3dmol": "^2.5.3",
    "axios": "^1.11.0",
    "chart.js": "^4.5.0",
    "firebase": "^12.0.0",
    "formik": "^2.4.6",
    "jquery": "^3.7.1",
    "react": "^18.2.0",
    "react-chartjs-2": "^5.3.0",
    "react-dom": "^18.2.0",
    "react-icons": "^4.10.1",
    "react-router-dom": "^6.26.2",
    "react-scripts": "^5.0.1",
    "react-toastify": "^11.0.5",
    "three": "^0.180.0",
    "web-vitals": "^2.1.4",
    "yup": "^1.7.0"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:e2e:headed": "playwright test --headed",
    "test:e2e:debug": "playwright test --debug",
    "test:e2e:report": "playwright show-report",
    "test:module1": "playwright test tests/module-1-login.spec.js --project=chromium --reporter=html",
    "test:module2": "playwright test tests/module-2-register.spec.js --project=chromium --reporter=html",
    "test:module3": "playwright test tests/module-3-concepts.spec.js --project=chromium --reporter=html",
    "test:module4": "playwright test tests/module-4-quiz-scoring.spec.js --project=chromium --reporter=html",
    "test:module5": "playwright test tests/module-5-misconception.spec.js --project=chromium --reporter=html",
    "test:module6": "playwright test tests/module-6-performance.spec.js --project=chromium --reporter=html",
    "test:all-modules": "playwright test tests/module-1-login.spec.js tests/module-2-register.spec.js tests/module-3-concepts.spec.js tests/module-4-quiz-scoring.spec.js tests/module-5-misconception.spec.js tests/module-6-performance.spec.js --project=chromium --reporter=html",
    "test:generate-reports": "node scripts/generate-module-reports.js",
    "allure:generate": "allure generate -c allure-results -o allure-report",
    "allure:open": "allure open allure-report",
    "report:export": "node scripts/export-allure-csv.js",
    "report:export:pdf": "node scripts/export-allure-csv.js --pdf"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@playwright/test": "^1.56.1",
    "allure-commandline": "^2.30.0",
    "allure-playwright": "^2.15.0",
    "puppeteer": "^23.6.0"
  }
}
